{
  "name": "ugo-playground",
  "version": "0.2.5",
  "private": true,
  "description": "uGO Playground",
  "author": {
    "name": "Ozan Hacıbekiroğlu"
  },
  "scripts": {
    "serve": "make development && vue-cli-service serve",
    "build": "make production && vue-cli-service build",
    "lint": "make development && vue-cli-service lint --fix",
    "test:unit": "vue-cli-service test:unit",
    "test": "vue-cli-service test:unit"
  },
  "main": "index.js",
  "dependencies": {
    "comlink": "^4.3.1",
    "core-js": "^3.15.2",
    "mini-toastr": "^0.8.1",
    "prismjs": "git+https://github.com/ozanh/prism.git",
    "vue": "^3.2.33",
    "vue-prism-editor": "^2.0.0-alpha.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.14.8",
    "@vue/cli-plugin-babel": "^5.0.4",
    "@vue/cli-plugin-eslint": "^5.0.4",
    "@vue/cli-plugin-unit-jest": "^5.0.4",
    "@vue/cli-service": "^5.0.4",
    "@vue/compiler-sfc": "^3.1.5",
    "@vue/eslint-config-standard": "^6.1.0",
    "@vue/test-utils": "^2.0.0-rc.10",
    "@vue/vue3-jest": "^27.0.0",
    "babel-jest": "^27.5.1",
    "copy-webpack-plugin": "^10.2.4",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-vue": "^7.20.0",
    "file-loader": "^6.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.1.0",
    "jest-raw-loader": "^1.0.1",
    "license-checker-webpack-plugin": "^0.2.1",
    "raw-loader": "^4.0.2",
    "regenerator-runtime": "^0.13.9",
    "sass": "^1.36.0",
    "sass-loader": "^12.6.0",
    "typescript": "^4.6.4",
    "vue-loader": "^17.0.0",
    "webpack": "^5.72.0",
    "worker-loader": "^3.0.8"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended",
      "@vue/standard"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ],
    "ignorePatterns": [
      "wasm_exec.*js"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      ".*wasm\\.worker": "<rootDir>/__mocks__/wasm.worker.js",
      "mini-toastr": "<rootDir>/__mocks__/mini-toastr.js",
      ".+\\.(css|less)$": "identity-obj-proxy",
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      ".*\\.(vue)$": "@vue/vue3-jest",
      ".*\\.(js|jsx)$": "babel-jest",
      ".*\\.ugo$": "jest-raw-loader"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "testMatch": [
      "<rootDir>/tests/unit/**.spec.js"
    ],
    "preset": "@vue/cli-plugin-unit-jest"
  },
  "babel": {
    "presets": [
      "@vue/cli-plugin-babel/preset"
    ]
  },
  "keywords": [
    "uGO"
  ],
  "license": "MIT"
}
